<nav class="login_navbar navbar navbar-inverse navbar-fixed-top navbar-top"> 
    <div class="container-fluid">
        <div class="navbar-header">
            <div class="applogo">
                <div class="logo">
                    <img src="/../../img/charterglobal_logo.jpg" alt="Charter Global">
                </div>
            </div>
            <ol class="breadcrumb navbar-breadcrumb">
                <li class="active">IT Help Desk</li>
            </ol>
        </div>
    </div>
</nav>
<!--ng-controller="loginAppCtrl"-->
<div class="login-box container" ng-controller="loginAppCtrl">
    <div class="col-xs-6 col-xs-offset-3">
        <div class="card login-form">
            <div class="card-header">
                <div class="card-title">
                    <div class="title">View Your Ticket</div>
                </div>
            </div>
            <div class="card-body">
                <div class="progress" id="login-progress" ng-class="{
                            hidden:!loginProgress
                        }">
                    <div class="progress-bar progress-bar-success progress-bar-striped active" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                        Log In...
                    </div>
                </div>
                <div class="alert alert-danger col-md-12" id="danger-alert" style="display: none">
                    <!--                    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>-->
                    <center> <strong> Oops! </strong><br> Reason: {{errorMsg}} </center>
                </div>
                <div class="alert alert-warning"  ng-show="showreloginMsg"  role="alert">
                    Password Changed Successfully Please Re-login Again
                </div>
                <form class="form-horizontal" ng-submit="showstatus(data)" ng-init="loadTicketsFrom();">
                    <div class="form-group">
                        <label for="emp_ticketid" class="col-sm-2 control-label">Ticket-ID</label>
                        <div class="col-sm-10">
                            <input type="TicketID" class="form-control" id="emp_ticketid" placeholder="TicketID" ng-model="data.ticketid" required>
                        </div>
                    </div>
                     <div class="form-group">
                        <label for="emp_emailid" class="col-sm-2 control-label">Email</label>
                        <div class="col-sm-10">
                            <input type="email" class="form-control" id="emp_emailid" placeholder="Email" ng-model="data.emailid" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">
                            <button type="submit" class="btn btn-primary" value="Submit">Submit</button>
                        </div>
                    </div>
                      <div class="text-right">
                    <span class="text-right"><a href="/login" class="">Login</a></span>
                </div>
                </form>
            </div>
        </div>
    </div>
</div>




                              
